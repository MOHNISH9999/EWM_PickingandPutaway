            android:layout_height="wrap_content"
            app:keyText="WarehouseOrder"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.warehouseOrder).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="IsCountCompleted"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.isCountCompleted).toString()}" />

    </LinearLayout>
</layout>