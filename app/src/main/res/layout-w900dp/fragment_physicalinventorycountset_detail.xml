        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Unit"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.unit).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Quantity"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.quantity).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="ExceptionCode"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.exceptionCode).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Queue"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.queue).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Itemtype"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.itemtype).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="IsEmptyBin"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.isEmptyBin).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
