<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".mdui.physicalinventorycountset.PhysicalInventoryCountSetActivity">
    <data>
        <import type = "com.sap.cloud.android.odata.crvwm_physicalinventory_srv_entities.PhysicalInventoryCount" />
        <variable name="physicalInventoryCount" type="PhysicalInventoryCount"/>
        <variable name="handler" type="com.company.ewm_pickingandputaway.mdui.physicalinventorycountset.PhysicalInventoryCountSetDetailFragment" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="PhyInvDocGUID"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.phyInvDocGUID).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="PhyInvDocument"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.phyInvDocument).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="PhyInvDocItem"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.phyInvDocItem).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="PhyInvDocYear"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.phyInvDocYear).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="WarehouseNumber"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.warehouseNumber).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="DocumentType"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.documentType).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="ProposeIndicator"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.proposeIndicator).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="BlockingIndicator"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.blockingIndicator).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="FreezeBookInvInd"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.freezeBookInvInd).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="StorageBin"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.storageBin).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Product"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.product).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="HandlingUnit"
            app:valueText="@{physicalInventoryCount.getOptionalValue(PhysicalInventoryCount.handlingUnit).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
